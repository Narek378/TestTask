<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Picsart Test Suite" parallel="tests" thread-count="3">
    <listeners>
        <listener class-name="io.qameta.allure.testng.AllureTestNg"/>
    </listeners>
    <test name="SearchTest(1024x768)">
        <parameter name="width" value="1024" />
        <parameter name="height" value="768" />
        <classes>
            <class name="com.picsart.tests.SearchTest" />
        </classes>
    </test>
    <test name="SearchTest(1440x900)">
        <parameter name="width" value="1440" />
        <parameter name="height" value="900" />
        <classes>
            <class name="com.picsart.tests.SearchTest" />
        </classes>
    </test>
    <test name="SearchTest(1366x768)">
        <parameter name="width" value="1366" />
        <parameter name="height" value="768" />
        <classes>
            <class name="com.picsart.tests.SearchTest" />
        </classes>
    </test>
</suite>
